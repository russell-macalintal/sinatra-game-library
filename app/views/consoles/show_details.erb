<h2>Console Details</h2>
<h4>Name: <%= @console.name %></h4>
<h4>Manufacturer: <%= @console.manufacturer %></h4>
<h4>Owners: </h4>
<ul>
  <% @console.users.each do |user| %>
    <li><%= user.username %></li>
  <% end %>
</ul>
<br>
<% if current_user.username == "ADMIN" %>
  <h4><a href="/consoles/<%= @console.id %>/edit">Edit/Delete Console</a></h4>
<% end %>
